#QUESTION 3

import random
correct = False
while(correct != True):
    length = int(input("INPUT THE LENGTH OF YOUR PASSWORD: "))
    
    if(length >= 0):
        correct = True
        
    else:
        print("INVALID INPUT, CHOOSE AGAIN!")

finalPw = ''
for i in range(0, length):
    thisChar = random.randint(97,122)
    thisChar = chr(thisChar)
    finalPw += thisChar
    
    
print("FINAL GENERATED PASSWORD WITH RANDOM CHARACTERS: ", finalPw)
    
    


